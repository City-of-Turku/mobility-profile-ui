{
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "lib": ["ESNext", "dom"],
    "jsx": "react",
    "allowJs": true,
    "noEmit": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true
  },
  "include": ["src"]
}
